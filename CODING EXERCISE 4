package selfprog;
import java.util.Scanner;
public class NO4 {
public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	int total=0,i=0;
	System.out.println("enter mark");
	while(i<5) {
	
	int mark=sc.nextInt();
	
	total=total+mark;
	
	i=i+1;
	}
	System.out.println("total: "+ total);
}
}
